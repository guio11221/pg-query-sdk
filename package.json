{
  "name": "pg-query-sdk",
  "version": "1.2.1",
  "description": "PostgreSQL SDK with Query Builder, Query Executor and Transaction Manager",
  "type": "commonjs",
  "main": "dist/cjs/index.js",
  "types": "dist/esm/index.d.ts",
  "sideEffects": false,
  "engines": {
    "node": ">=18"
  },
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js",
      "types": "./dist/esm/index.d.ts"
    }
  },
  "scripts": {
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build": "npm run build:cjs && npm run build:esm",
    "lint": "echo \"Add lint later\"",
    "test": "echo \"Add tests\"",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "postgresql",
    "postgres",
    "typescript",
    "nodejs",
    "database",
    "sql",
    "query-builder",
    "sdk",
    "orm",
    "transaction",
    "cte",
    "repository-pattern",
    "query-engine",
    "prepared-statement"
  ],
  "author": {
    "name": "Guilherme",
    "email": "sguii5147@gmail.com",
    "url": "https://github.com/guio11221"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guio11221/pg-query-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/guio11221/pg-query-sdk/issues"
  },
  "homepage": "https://github.com/guio11221/pg-query-sdk#readme",
  "license": "MIT",
  "dependencies": {},
  "peerDependencies": {
    "pg": "^8.0.0"
  },
  "devDependencies": {
    "typescript": "^5.3.3",
    "@types/node": "^20.10.5",
    "@types/pg": "^8.16.0",
    "pg": "^8.18.0"
  }
}